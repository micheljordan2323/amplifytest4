# Amplify Gen2 Configuration
NEXT_PUBLIC_AMPLIFY_PROJECT_NAME=amplifytest4
NEXT_PUBLIC_AMPLIFY_PROJECT_ENV=dev

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# AWS Bedrock Configuration
BEDROCK_ENABLED=true
BEDROCK_MODEL_IDS=anthropic.claude-3-sonnet-20240229-v1:0,anthropic.claude-3-haiku-20240307-v1:0,anthropic.claude-3-opus-20240229-v1:0

# Rate Limiting
BEDROCK_RATE_LIMIT_REQUESTS=10
BEDROCK_RATE_LIMIT_WINDOW_MS=60000

# Application Configuration
NEXT_PUBLIC_APP_NAME="AI Chat App"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# Development Configuration
NODE_ENV=development 